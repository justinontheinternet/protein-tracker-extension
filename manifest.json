{
  "manifest_version": 2,

  "name": "Protein Tracker",
  "description": "Helps you set a goal for, and track your protein intake.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "options_page": "options.html",

  "permissions": [
    "storage"
  ]
}